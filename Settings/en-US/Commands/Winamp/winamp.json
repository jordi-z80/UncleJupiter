[
	{
		// the name of the action. Must be unique.
		"name":"winamp_next_song",
		
		// The regexps to match with quick run
		"regExp": 
		[
			"Next song",
		],
		
		// List of words that are banned from quick run but we use.
		"allowedWords":
		[
			"Next",
		],
		
		// only when WinAmp is running
		"isProcessRunning": "winamp",
		
		// action and param translation of the regEx if it matches.
		"action": "%WINAMP%",
		"param": "/NEXT",
		
		// descriptive text for the LLM
		"llmText":"If the user wants to play the next song (and he's not searching nor specifying a song), use action = '%WINAMP%' and param = '/NEXT'."
	}
